groups:
- name: HTTP-SVC-THRESHOLD
  rules:
  - alert: LessThanONEalertmanager
    expr: up{job="alertmanager"} == 0 and on(job) sum by(job) (up{job="alertmanager"}) <= 1
    for: 10s
    labels:
      severity: "P2"
    annotations:
      summary: "There is no alert managers :("
      description: "The service {{ $labels.job }} on instance {{ $labels.instance }} is completely down "


